<style type="text/css">
    body{margin: 0; background-color: rgb(138, 170, 229, .8)}
    #page_banner{background-color: white; padding: 15px; border: solid 0.5px; color: 8AAAE5; font-size: 20px; padding-left: 30px; font-family: Inter, sans-serif;}
    tr{text-align:center}
    #table_header{color: FFFFFF; font-size: 32px; font-family: Inter, sans-serif;}
    #insurance_plans{margin-left: auto; margin-right: auto; margin-top: 50px; text-align: center;  margin-bottom: 50px }
    tbody{background-color: white; text-align:center; display:inline-block; padding-left: 100px; padding-right: 100px; margin-top: 30px}
    tbody tr{background-color: A0CFB4; margin-top: 15px; margin-bottom: 15px; display:block; padding-right: 80px; padding-left: 80px; padding-top: 30px; padding-bottom: 30px}
</style>

<div id="page_banner">EnsurePrice</div>
<table class="table table-striped col-md-12" id="insurance_plans">
  <thead>
  <tr>
    <th id="table_header">Select Your Insurance Plan<th>
  </tr>
  </thead>
  <tbody>
  <% @insurance_plans.each do |insurance_plan| %>
    <tr>
      <td>
        <%= link_to insurance_plan, doctors_path(insurance_plan) %>
      </td>
    </tr>
  <% end %>
  </tbody>
</table>