
<table class="table table-striped col-md-12" id="table-intro">
  <thead>
    <tr>
      <th id="table_header">Select Your Insurance Provider<th>
    </tr>
  </thead>
  <table>
    <% @insurance_providers.each do |insurance_provider| %>
      <div class="row">
        <div class="col-md-1" id="insurance-logos">
          <%= image_tag InsuranceProviderLogo.get_logo_url(insurance_provider), id: "insurance-logo" %>
        </div> 
        <div class="col-md-11" id="insurance-names">    
          <%= button_to insurance_provider, plans_path(insurance_provider), id: "insurance-provider-btn", method: :get %>
        </div>
      </div>
      <br>
    <% end %>
  </table>
</table>
<br><br>
<%= link_to '<button class="btn btn-secondary">Back To Home</button>'.html_safe, '/' %>

