
<div class="container-fluid" id="add-review-container">
  <div id="doctor-profile-header">Edit your Question</div>
  <br>
  <div class="container-fluid">
    <%= form_for(@question, as: :question) do |f| %>
      <%= render 'shared/error_question_messages' %>
      <%= f.text_area :ques, :rows => '2', :class => 'form-control' %><br>
      <%= f.submit "Update My Question", class: "btn btn-primary" %>
    <% end %>  
  </div>
  <br>
  <%= link_to '<button class="btn btn-secondary back-button">Back to FAQ</button>'.html_safe, questions_path %>
</div>
